@model BelarusLib.Models.TypeComposition
@{
    ViewBag.Title = "Удаление типа произведения";
}

<h1>Вы дакладна хочаце выдаліць тып твора: <b>@Model.TypeCompositionName</b>?</h1>

@using (Html.BeginForm())
{
    <div class="form-group">
        <input type="submit" value="Выдалiць" class="btn btn-danger" />
        <a class="btn btn-default" href="@Url.Action("GetTypeComposition", "Admin")">Назад</a>
    </div>
}
